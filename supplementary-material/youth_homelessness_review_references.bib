@article{sullivan2023impactdomesticviolence,
  title = {Impact of the {{Domestic Violence Housing First Model}} on {{Survivors}}' {{Safety}} and {{Housing Stability}}: 12-{{Month Findings}}},
  shorttitle = {Impact of the {{Domestic Violence Housing First Model}} on {{Survivors}}' {{Safety}} and {{Housing Stability}}},
  author = {Sullivan, Cris M. and Guerrero, Mayra and Simmons, Cortney and {L{\'o}pez-Zer{\'o}n}, Gabriela and Ayeni, Oyesola Oluwafunmilayo and Farero, Adam and Chiaramonte, Danielle and Sprecher, Mackenzie},
  year = {2023},
