@article{tucker2016251briefgroupmi,
  title = {251 {{A BRIEF GROUP-MI INTERVENTION REDUCES ALCOHOL USE AMONG HOMELESS YOUNG ADULTS}}},
  author = {Tucker, Joan S. and D'Amico, Elizabeth J. and Ewing, B. A. and Pedersen, Eric R.},
  year = {2016},
  month = jun,
  journal = {Alcoholism: Clinical and Experimental Research},
  volume = {40},
  number = {S1},
  pages = {311A},
  issn = {0145-6008, 1530-0277},
  doi = {10.1111/acer.13085},
  urldate = {2024-11-05},
  abstract = {Methods: Individuals (n = 135) with alcohol dependence, who had recent alcohol use (within 30 days) were enrolled. Subjects stratified by smoking status (79 = smokers, 56 = nonsmokers) were randomized in a double blind fashion to receive mecamylamine (10 mg/day as 2.5 mg tablets, two tablets po bid) or matching placebo (two placebo tablets po bid) for 12 weeks; all participants also received Medical Management administered by an experienced research registered nurse. Results: All drinking outcomes (heavy drinking days,\# drinking days, drinks per drinking day) decreased significantly with treatment p {$<$}= 0.01. Rate of abstinence in this study was low (26.5\%) with no differences between medication groups (p = 0.61) or between smokers and non-smokers (p = 0.89). There was no main effect for medication on drinking days (p = 0.96), drinks per drinking days (p = 0.83), or heavy drinking days (p = 0.91). There was a signficant main effect of smoking for drinking days (p = 0.05) (smokers reported fewer drinking days), but not for heaving drinking days or drinks per drinking days. There was no significant medication by smoking interaction for drinking days (p = 0.80), heavy drinking days (0.84) or drinks per drinking days (p = 0.65). The average number of cigarrets per day was lower during treatment compared to baseline, atlhought this was not statistically significantly. There were no differences in the number of cigarrettes smoked between the mecamylamine group (mean = 9.4) and the placebo group (mean = 11.1). Other secondary outcomes and possible moderators, such as alcohol and smoking characteristics will be explored. Conclusion: Preliminary analyses suggest that the non-competitive nicontinic antagonist, mecamylamine, was not more effective than placebo in reducing drinking and was not associated with an increase in smoking. This work was supported by NIAAA RO1 AA016834},
  copyright = {http://onlinelibrary.wiley.com/termsAndConditions\#vor},
  langid = {english},
  file = {/Users/dtay0016/Zotero/storage/N69BEFYY/Tucker et al. - 2016 - 251 A BRIEF GROUP-MI INTERVENTION REDUCES ALCOHOL .pdf}
}

@article{tucker2017groupbasedmotivationalinterviewing,
  title = {A Group-Based Motivational Interviewing Brief Intervention to Reduce Substance Use and Sexual Risk Behavior among Homeless Young Adults},
  author = {Tucker, Joan S. and D'Amico, Elizabeth J. and Ewing, Brett A. and Miles, Jeremy N.V. and Pedersen, Eric R.},
  year = {2017},
  month = may,
  journal = {Journal of Substance Abuse Treatment},
  volume = {76},
  pages = {20--27},
  issn = {07405472},
  doi = {10.1016/j.jsat.2017.02.008},
  urldate = {2024-11-04},
  abstract = {Homeless young adults ages 18--25 exhibit high rates of alcohol and other drug (AOD) use, and sexual risk behaviors such as unprotected sex. Yet few programs exist for this population that are both effective and can be easily incorporated into settings serving this population. This pilot cluster cross-over randomized controlled trial evaluates AWARE, a voluntary four session group-based motivational interviewing (MI) intervention to reduce AOD use and sexual risk behavior. We evaluated AWARE with 200 homeless young adults using drop-in center services in Los Angeles County (mean age = 21.8 years; 73\% male; 79\% heterosexual; 31\% non-Hispanic White, 25\% African American, 24\% Hispanic, 21\% multiracial/other). Surveys were completed at baseline and three months after program completion. Retention in the AWARE program was excellent (79\% attended multiple sessions) and participants reported high levels of satisfaction with the program. AWARE participants self-reported positive change in their past 3 month and past 30 day alcohol use (ps {$\leq$} 0.05), motivation to change drug use (ps b 0.05), and condom use self-efficacy (p = 0.05) compared to the control group. Among those with multiple sex partners, AWARE participants showed a decrease in unprotected sexual events (p b 0.05), whereas the control group did not. Results from this pilot evaluation are promising, suggesting that a brief group-MI risk reduction intervention can be effective in helping homeless young adults make positive changes in their alcohol and condom use. Further work is needed to more fully evaluate the efficacy of AWARE on AOD behavior and sexual risk behavior outcomes.},
  langid = {english},
  file = {/Users/dtay0016/Zotero/storage/AG76V24S/Tucker et al. - 2017 - A group-based motivational interviewing brief inte.pdf}
}

@article{tucker2020studyprotocolgroupbased,
  title = {Study Protocol for a Group-Based Motivational Interviewing Brief Intervention to Reduce Substance Use and Sexual Risk Behavior among Young Adults Experiencing Homelessness},
  author = {Tucker, Joan S. and D'Amico, Elizabeth J. and Pedersen, Eric R. and Rodriguez, Anthony and Garvey, Rick},
  year = {2020},
  month = dec,
  journal = {Addiction Science \& Clinical Practice},
  volume = {15},
  number = {1},
  pages = {26},
  issn = {1940-0640},
  doi = {10.1186/s13722-020-00201-x},
  urldate = {2024-11-04},
  abstract = {Abstract                            Background               Young people experiencing homelessness have alarmingly high rates of alcohol and other drug (AOD) use, which is associated with sexual risk behaviors such as unprotected sex, trading sex, and sex with multiple casual partners. Few risk reduction programs for this population have been developed and rigorously evaluated, particularly those that address both of these interrelated behaviors, use a collaborative and non-judgmental approach, and are feasible to deliver in settings where homeless young people seek services. This paper describes the protocol of a study evaluating a four-session Motivational Interviewing (MI)-based group risk reduction intervention for this population. The protocol has been shown to be efficacious in pilot work over 3 months with 200 homeless young adults [1]. The current study seeks to refine the intervention protocol and evaluate the program on a larger scale.                                         Methods/design               In a cluster-cross-over randomized controlled trial, 18--25~year olds will receive the AWARE risk reduction program (n\,=\,200) or standard care (n\,=\,200) at one of three drop-in centers serving homeless youth in the Los Angeles area. We will evaluate intervention effects on primary outcomes of AOD use and sexual risk behavior, as well as secondary outcomes of health-related quality of life and social stability, over a 12-month period.                                         Discussion               This project has the potential to fill a significant gap in prevention services by demonstrating that a brief intervention, feasible to deliver within settings where young people experiencing homelessness typically seeks services, can significantly reduce the interrelated problems of AOD use and sexual risk behavior.                                Trial registration                 ClinicalTrials.gov Identifier: NCT03735784. Registered November 18, 2018,                 https://clinicaltrials.gov/ct2/show/record/NCT03735784                 (retrospectively registered)},
  langid = {english},
