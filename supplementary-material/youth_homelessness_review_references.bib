@article{krabbenborg2017clusterrandomizedcontrolled,
  title = {A {{Cluster Randomized Controlled Trial Testing}} the {{Effectiveness}} of {{Houvast}}: {{A Strengths-Based Intervention}} for {{Homeless Young Adults}}},
