@article{kozloff201622housingfirst,
  title = {3.22 {{HOUSING FIRST FOR HOMELESS YOUTH WITH MENTAL ILLNESS}}: {{ANALYSIS FROM A RANDOMIZED CONTROLLED TRIAL}}},
  shorttitle = {3.22 {{HOUSING FIRST FOR HOMELESS YOUTH WITH MENTAL ILLNESS}}},
  author = {Kozloff, Nicole and Stergiopoulos, Vicky and Cheung, Amy and Goering, Paula~N.},
  year = {2016},
  month = oct,
  journal = {Journal of the American Academy of Child \& Adolescent Psychiatry},
  volume = {55},
  number = {10},
  pages = {S149},
  issn = {08908567},
  doi = {10.1016/j.jaac.2016.09.154},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S0890856716313831},
  urldate = {2023-07-27},
  abstract = {Objectives: Given the lack of evidence for interventions targeting housing outcomes in transitional-age homeless youth (ages 18{\textendash}24 years), we aimed to test the effect of Housing First on housing stability among a group of homeless youth with mental illness enrolled in At Home/Chez Soi, a \$110 million 24month randomized controlled trial of Housing First in five Canadian cities. Methods: Homeless individuals with mental illness were randomized to receive Housing First, a scattered-site choice-based housing intervention (without preconditions of sobriety or psychiatric treatment) and mental health services according to the level of need (assertive community treatment or intensive case management), or usual care. Participants completed interviews every 6 months and measures of housing status every 3 months to a total of 24 months. We assessed housing stability as percent days stably housed as a proportion of days for which residence data were available in youth aged 18{\textendash}24 years, fit a model with a three-way interaction of treatment group (age 18{\textendash}24 years vs. {$>$}24 years), and compared the effect on housing stability in youth versus adults. We used a repeated measures mixed-effects model to estimate differences/ratios from baseline over 24 months for secondary health and service use outcomes. Results: Of the 2,148 total participants who were randomized, 156 were youth aged 18{\textendash}24 years: 87 received Housing First and 69 received usual care. In an adjusted analysis, youth in Housing First were stably housed a mean of 437/645 (65 percent) days for which data were available compared with youth in usual care, who were stably housed a mean of 189/582 (31 percent) days, resulting in an adjusted mean difference of 34 percent (95\% CI 24{\textendash}45, P {$<$} 0.0001). The effect of the three-way interaction of treatment group, age, and time on housing stability was nonsignificant (P {$\frac{1}{4}$} 0.5934). Youth who received Housing First were less likely to have a period of employment (adjusted OR {$\frac{1}{4}$} 0.48, 95\% CI 0.24{\textendash}0.99, P {$\frac{1}{4}$} 0.0456), but otherwise, their secondary outcomes were not significantly different from youth who received usual care. Conclusions: Housing First led to improved housing stability in homeless youth with mental illness and appears to be no less effective in youth than in older adults. More research is needed to guide adaptations of the model to further improve housing stability and other outcomes in youth.},
  langid = {english},
  file = {/Users/davetaylor/Zotero/storage/E73UUAT6/Kozloff et al. - 2016 - 3.22 HOUSING FIRST FOR HOMELESS YOUTH WITH MENTAL .pdf}
}

@article{kozloff2016uniqueneedshomeless,
  title = {The {{Unique Needs}} of {{Homeless Youths With Mental Illness}}: {{Baseline Findings From}} a {{Housing First Trial}}},
  shorttitle = {The {{Unique Needs}} of {{Homeless Youths With Mental Illness}}},
  author = {Kozloff, Nicole and Stergiopoulos, Vicky and Adair, Carol E. and Cheung, Amy H. and Misir, Vachan and Townley, Greg and Bourque, Jimmy and Krausz, Michael and Goering, Paula},
  year = {2016},
