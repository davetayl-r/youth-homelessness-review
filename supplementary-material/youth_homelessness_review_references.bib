@article{mallory2022variabilitytreatmentoutcomes,
  title = {Variability in treatment outcomes from a housing intervention for young mothers misusing substances and experiencing homelessness by sexual identity},
  author = {Mallory, Allen B. and Luthy, Ellison and Martin, Jared K. and Slesnick, Natasha},
  year = {2022},
  month = aug,
  journal = {Children and Youth Services Review},
  volume = {139},
